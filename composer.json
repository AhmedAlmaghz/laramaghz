{
    "name": "almaghz/laramaghz",
    "description": "laraMaghz package - laravel Admin,web and API",
    "license": "MIT",
    "keywords": ["laravel","laravel Admin","laramaghz","maghz","admin","web","api","crud"],
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Ahmed almaghz",
            "email": "almaghz@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "laravelcollective/html":"^6.1.2",
        "mcamara/laravel-localization": "^1.5",
        "madnest/madzipper": "1.0.x",
        "laravel/ui": ">=2.0",
        "jeroennoten/laravel-adminlte":"~3.0"

    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "orchestra/testbench-browser-kit": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Almaghz\\Laramaghz\\": "src/"

        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Almaghz\\Laramaghz\\Providers\\LaramaghzServiceProvider"
            ]
        }
    }
}
